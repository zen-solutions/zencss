{
    "manifest_version": 2,
    "name": "zenCSS Test Tool",
    "version": "1.0",
    "description": "A CSS testing and debugging tool for zenCSS",
    "permissions": ["activeTab", "storage"],
    "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"],
        "run_at": "document_end"
      }
    ],
    "web_accessible_resources": [
      "zen-test.css"
  ]
  }
  